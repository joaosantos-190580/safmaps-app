		(function() {
			// VARIABLE STATEMENTS
			var windowHeight = getActualHeight(), windowWidth = getActualWidth();

			var route = '', feedstock = '';
			var revap = '', espigao = '';
			var capacidadeEucalipto_valor = '';
			var revapSoja = '', rnestSoja = '';
			var brumadoSoja = '', paranaibaSoja = '', pVenceslauSoja = '';
			var prataCana = '', cacuCana = '', paranaibaCana = '', pVenceslauCana = '';

			var routeSoja = '', feedstockSoja = '';
			var selecionadoSoja = '', tipoInstalacaoSoja = '', capacidadeSoja = '', capacidadeSoja_valor = '';
			var locationSoja = '', oilSourceSoja = '';
			var inputReqCalc_soja = '';

			var routeMacauba = '', feedstockMacauba = '';
			var selecionadoMacauba = '', tipoInstalacaoMacauba = '', capacidadeMacauba = '';
			var locationMacauba = '', oilSourceMacauba = '';

			var routePalma = '', feedstockPalma = '';
			var selecionadoPalma = '', tipoInstalacaoPalma = '', capacidadePalma = '';
			var locationPalma = '', oilSourcePalma = '';			

			var routeCana = '', feedstockCana = '', feedstockCana_valor = '';
			var selecionadoCana = '', tipoInstalacaoSugarcane = '', tipoInstalacaoSugarcane_valor = '', capacidadeCana = '', capacidadeCana_valor = '';
			var locationCana = '', productionCana = '', productionCana_valor = '';

			var routeMilho = '', feedstockMilho = '', feedstockMilho_valor = '';
			var selecionadoMilho = '', tipoInstalacaoMilho = '', tipoInstalacaoMilho_valor = '', capacidadeMilho = '', capacidadeMilho_valor = '';
			var locationMilho = '', productionMilho = '', productionMilho_valor = '';

