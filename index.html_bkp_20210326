  <script>
		var current_tab = 'physical', last_tab = '';

		function setCurrentTab() {
			current_tab = $("widget-tab").prevObject[0].URL;
			current_tab = current_tab.substring(current_tab.indexOf("#"));
			console.log(current_tab);

			if (last_tab === '') {
				last_tab = current_tab;
				$(current_tab).toggleClass('redux-other-panels');
			} else if (last_tab !== current_tab) {
				$(last_tab).toggleClass('redux-other-panels');
				last_tab = current_tab;
				$(current_tab).toggleClass('redux-other-panels');				
			} else if (last_tab === current_tab) {
				$(last_tab).toggleClass('redux-other-panels');
				last_tab = '';
			}
		}
  </script>

