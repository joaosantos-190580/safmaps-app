  <script>
	// First opened panel
	var current_panel = 'political';

    // Button #menu-toggle
    $("#menu-toggle").click(function(e) {
        e.preventDefault();
        $("#wrapper").toggleClass("toggled");
        if ($("#opened").css("display") === "block") {
            $("#opened").css("display", "none");
            $("#closed").css("display", "block");
        } else {
            $("#closed").css("display", "none");
            $("#opened").css("display", "block");
        }
    });

	//SUPPORT MAPS
    $("#support-maps").click(function(e) {
        e.preventDefault();

        reset_actived (e);
		$("#panel-eucalipto").css("display", "none");
		$("#panel-eucalipto-cstudies").css("display", "none");
		$("#panel-soja").css("display", "none");
		$("#panel-soja-cstudies").css("display", "none");
		$("#panel-macauba").css("display", "none");
		$("#panel-macauba-cstudies").css("display", "none");
		$("#panel-palma").css("display", "none");
		$("#panel-palma-cstudies").css("display", "none");		
		$("#panel-sugarcane").css("display", "none");	
		$("#panel-sugarcane-cstudies").css("display", "none");		
		$("#panel-corn").css("display", "none");		
		$("#panel-corn-cstudies").css("display", "none");		
