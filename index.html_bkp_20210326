          if($(this).prop("checked") == true) {
              //var prov = L.tileLayer.wms('http://35.247.202.68/geoserver/ows?', { layers: l_produtividade_soja, format: 'image/png', transparent: true });
		  	  var prov = L.tileLayer.wms(url, options);   
			  map.addLayer(prov);

              $("#legend-produtividade_soja").css("display", "block");
          } else {
              $("#legend-produtividade_soja").css("display", "none");
          }
	  });

      // Cost of soybean production (Layer)
	  $("#toggle-custo_soja").on('change', function(){
          $('input:checkbox').not(this).prop('checked', false);
          reset_all_legends();
          removeLayers();

		  options['layers'] = l_custos_soja;

          if($(this).prop("checked") == true) {
              //var prov = L.tileLayer.wms('http://35.247.202.68/geoserver/ows?', { layers: l_custos_soja, format: 'image/png', transparent: true });
		  	  var prov = L.tileLayer.wms(url, options);   
			  map.addLayer(prov);

              $("#legend-custo_soja").css("display", "block");
          } else {
              $("#legend-custo_soja").css("display", "none");
          }
	  });


